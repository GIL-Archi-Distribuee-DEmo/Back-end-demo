type Query{
    courses:[Course]
    course(id:ID):Course !
}
type Mutation{
    newCourse(course:CourseInput!):Course
}
input CourseInput{
    course_id:ID !
    name: String!   

}

type Course{
    course_id: ID!
    name: String!
    description: String!
    price: Int!
    capacities: Int!
    course_type : CourseType
    course_language:CourseLanguage
}

type CourseType{

}